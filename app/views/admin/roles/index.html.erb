<h1>Roles</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @roles.each do |role| %>
      <tr>
        <td><%= role.name %></td>
        <td><%= link_to 'Show', admin_role_path(role) %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br/>

<%= link_to 'Back', admin_path %>
